.tableList {
  width: 100%;
  height: 100rpx;
  padding: 0 30rpx;
}

.listItem {
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.edit {
  display: flex;
  align-items: center;
}

.select {
  color: rgba(255, 255, 255, .8);
  margin-right: 30rpx;
  padding: 8rpx;
  border-radius: 10rpx;
}

.del {
  color: rgba(255, 255, 255, .8);
  padding: 8rpx;
  border-radius: 10rpx;
}

