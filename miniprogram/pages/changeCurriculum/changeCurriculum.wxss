.tableList {
  width: 100%;
  height: 100rpx;
  padding: 0 30rpx;
}

.listItem {
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.edit {
  display: flex;
  align-items: center;
}

.select {
  color: rgba(255, 255, 255, .8);
  margin-right: 30rpx;
  padding: 8rpx;
  border-radius: 10rpx;
}

.del {
  color: rgba(255, 255, 255, .8);
  padding: 8rpx;
  border-radius: 10rpx;
}

.box {
  padding: 30rpx 30rpx 0rpx 30rpx;
  margin-bottom: 160rpx;
}

.box>input {
  border: 2rpx solid #000;
}

.tip {
  height: 30rpx;
}

.btnArea {
  margin-top: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.cl {
  margin-right: 60rpx;
}